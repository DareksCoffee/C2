enum WeekDay {
  Monday,
  Tuesday,
  Wednesday,
  Thursday,
  Friday,
  Saturday,
  Sunday,
}

function main() int {
  let day = WeekDay.Thursday;

  switch (day) {
    case (WeekDay.Monday, WeekDay.Tuesday, WeekDay.Wednesday, WeekDay.Thursday, WeekDay.Friday) {
      print("Work :(");
    },
    case (WeekDay.Saturday, WeekDay.Sunday) {
      print("Free from work!");
    }
  }

  let dayNumber: int = day;
  print(dayNumber); // 4

  return 0;
}
